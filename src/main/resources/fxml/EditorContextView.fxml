<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<BorderPane fx:id="pane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="wapuniverse.view.EditorContextController">
    <top>
        <VBox>
            <MenuBar>
                <Menu text="File"/>
                <Menu text="Edit">
                    <MenuItem onAction="#onDelete" text="Delete"/>
                </Menu>
            </MenuBar>
            <HBox>
                <ToggleButton fx:id="selectToolButton" text="S"/>
                <ToggleButton fx:id="moveToolButton" text="M"/>
                <ComboBox fx:id="activePlaneComboBox"/>
            </HBox>
        </VBox>
    </top>
    <center>
        <Text fx:id="imageSetText" text="WapUniverse"/>
    </center>
</BorderPane>
